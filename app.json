{
  "expo": {
    "name": "HabitFlow",
    "slug": "habitflow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "habitflow",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6200ee"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "b1aed33d-909c-4462-9167-0ea3647b389f"
      },
      "AI_SERVER_URL": "https://habit-ai-server.onrender.com"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kaustubhexpo.habitflow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6200ee"
      },
      "package": "com.kaustubhexpo.habitflow",
      "permissions": [
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#6200ee"
        }
      ],
      "expo-web-browser"
    ],
    "owner": "kaustubhexpo"
  }
}
